    "browserslist": "^4.21.4",
    "cypress": "^12.3.0",
    "cypress-wait-until": "^1.7.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.3.1",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
